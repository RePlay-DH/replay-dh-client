@prefix :        <http://www.ub.uni-stuttgart.de/replay#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix datacite:  <http://purl.org/spar/datacite/> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix p-plan:  <http://purl.org/net/p-plan#> .
@prefix prov:    <http://www.w3.org/ns/prov#> .

p-plan:hasOutputVar
      a       owl:ObjectProperty ;
      rdfs:domain p-plan:Step ;
      rdfs:range p-plan:Variable .

:resource2
      a       p-plan:Variable ;
      dcterms:identifier
              [ rdfs:label "model1" ;
                dcterms:type "Name-version"
              ] ;
      dcterms:identifier
              [ rdfs:label "path/to/model/dir/file1.xml" ;
                dcterms:type "Path"
              ] ;
      dcterms:type "inputResource" , "dataset/model" .

prov:SoftwareAgent
      a       owl:Class ;
      rdfs:subClassOf prov:Agent .

prov:AgentInfluence
      a       owl:Class ;
      rdfs:subClassOf prov:Influence .

prov:agent
      a       owl:ObjectProperty ;
      rdfs:domain prov:AgentInfluence ;
      rdfs:range prov:Agent .

prov:influenced
      a       owl:ObjectProperty ;
      rdfs:domain prov:Agent ;
      rdfs:range prov:Entity .

:person1
      a       prov:Person ;
      dcterms:identifier
              [ rdfs:label "约翰 2" ;
                dcterms:type "Name"
              ] .

:Activity_step_1
      a       p-plan:Activity , prov:Activity ;
      p-plan:correspondsToStep
              :step_1 .

:Activity_step_5
      a       p-plan:Activity , prov:Activity ;
      p-plan:correspondsToStep
              :step_5 ;
      prov:qualifiedAssociation
              :Association_tool1 .

:step_5
      a       p-plan:Step ;
      rdfs:label "analyze" ;
      dcterms:description "Some random description for step: analyze" ;
      p-plan:hasInputVar :resource2 , :resource3 , :resource1 ;
      p-plan:hasOutputVar :resource1 ;
      p-plan:isPreceededBy
              :step_4 ;
      p-plan:isStepOfPlan :Workflow ;
      prov:wasAssociatedWith
              :tool1 .

:resource3
      a       p-plan:Variable ;
      dcterms:identifier
              [ rdfs:label "model2" ;
                dcterms:type "Name-version"
              ] ;
      dcterms:identifier
              [ rdfs:label "path/to/model/dir/file2.xml" ;
                dcterms:type "Path"
              ] ;
      dcterms:type "inputResource" , "dataset/model" .

:Association_person2
      a       prov:Association ;
      prov:agent :person2 ;
      prov:hadRole "annotator" .

prov:Entity
      a       owl:Class ;
      owl:disjointWith prov:Person .

:step_1
      a       p-plan:Step ;
      rdfs:label "init" ;
      dcterms:description "Some random description for step: init" ;
      p-plan:hasInputVar :resource2 , :resource3 ;
      p-plan:isStepOfPlan :Workflow .

p-plan:Activity
      a       owl:Class ;
      rdfs:subClassOf prov:Activity .

prov:hadRole
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

prov:wasAssociatedWith
      a       owl:ObjectProperty ;
      rdfs:domain prov:Activity ;
      rdfs:range prov:Agent .

:person2
      a       prov:Person ;
      dcterms:identifier
              [ rdfs:label "约翰 1" ;
                dcterms:type "Name"
              ] .

:Activity_step_4
      a       p-plan:Activity , prov:Activity ;
      p-plan:correspondsToStep
              :step_4 ;
      prov:qualifiedAssociation
              :Association_tool1 .

dcterms:description
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

:Workflow
      a       p-plan:Plan .

p-plan:isStepOfPlan
      a       owl:ObjectProperty ;
      rdfs:domain p-plan:Step ;
      rdfs:range p-plan:Plan .

:Association_tool1
      a       prov:Association ;
      prov:agent :tool1 .

:tool1
      a       prov:SoftwareAgent ;
      dcterms:description "amd64" ;
      dcterms:identifier
              [ rdfs:label "myTool-v1" ;
                dcterms:type "Name-version"
              ] ;
      p-plan:hasInputVar "-v -file path/to/my/dir/model.xml" ;
      prov:influenced :Activity_step_5 , :Activity_step_6 , :Activity_step_4 .

rdfs:label
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

:step_6
      a       p-plan:Step ;
      rdfs:label "correct" ;
      dcterms:description "Some random description for step: correct" ;
      p-plan:hasInputVar :resource2 , :resource3 , :resource1 ;
      p-plan:hasOutputVar :resource1 ;
      p-plan:isPreceededBy
              :step_5 ;
      p-plan:isStepOfPlan :Workflow ;
      prov:wasAssociatedWith
              :tool1 , :person2 .

prov:qualifiedAssociation
      a       owl:ObjectProperty ;
      rdfs:domain prov:Activity ;
      rdfs:range prov:Association .

prov:Agent
      a       owl:Class .

:Association_person1
      a       prov:Association ;
      prov:agent :person1 ;
      prov:hadRole "annotator" .

prov:Association
      a       owl:Class ;
      rdfs:subClassOf prov:AgentInfluence .

p-plan:Plan
      a       owl:Class ;
      rdfs:subClassOf prov:Plan .

:step_2
      a       p-plan:Step ;
      rdfs:label "annotate" ;
      dcterms:description "Some random description for step: annotate" ;
      p-plan:hasInputVar :resource2 , :resource3 , :resource1 ;
      p-plan:hasOutputVar :resource1 ;
      p-plan:isPreceededBy
              :step_1 ;
      p-plan:isStepOfPlan :Workflow ;
      prov:wasAssociatedWith
              :person1 , :person2 .

p-plan:Variable
      a       owl:Class .

:Activity_step_3
      a       p-plan:Activity , prov:Activity ;
      p-plan:correspondsToStep
              :step_3 ;
      prov:qualifiedAssociation
              :Association_person2 .

prov:Person
      a       owl:Class ;
      rdfs:subClassOf prov:Agent .

dcterms:type
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

prov:Activity
      a       owl:Class .

p-plan:Step
      a       owl:Class .

prov:Influence
      a       owl:Class .

prov:Plan
      a       owl:Class .

:step_3
      a       p-plan:Step ;
      rdfs:label "curate" ;
      dcterms:description "Some random description for step: curate" ;
      p-plan:hasInputVar :resource2 , :resource1 ;
      p-plan:hasOutputVar :resource1 ;
      p-plan:isPreceededBy
              :step_2 ;
      p-plan:isStepOfPlan :Workflow ;
      prov:wasAssociatedWith
              :person2 .

p-plan:correspondsToStep
      a       owl:ObjectProperty ;
      rdfs:domain p-plan:Activity ;
      rdfs:range p-plan:Step .

dcterms:identifier
      a       owl:DatatypeProperty , owl:ObjectProperty .

:resource1
      a       p-plan:Variable ;
      dcterms:identifier
              [ rdfs:label "output/dir/result.xml" ;
                dcterms:type "Path"
              ] ;
      dcterms:type "inputResource" , "outputResource" , "dataset/analysis" .

:Activity_step_2
      a       p-plan:Activity , prov:Activity ;
      p-plan:correspondsToStep
              :step_2 ;
      prov:qualifiedAssociation
              :Association_person1 , :Association_person2 .

p-plan:hasInputVar
      a       owl:DatatypeProperty , owl:ObjectProperty ;
      rdfs:domain p-plan:Step ;
      rdfs:range xsd:string , p-plan:Variable .

:Activity_step_6
      a       p-plan:Activity , prov:Activity ;
      p-plan:correspondsToStep
              :step_6 ;
      prov:qualifiedAssociation
              :Association_person2 , :Association_tool1 .

:step_4
      a       p-plan:Step ;
      rdfs:label "train" ;
      dcterms:description "Some random description for step: train" ;
      p-plan:hasInputVar :resource2 , :resource3 , :resource1 ;
      p-plan:hasOutputVar :resource1 ;
      p-plan:isPreceededBy
              :step_1 ;
      p-plan:isStepOfPlan :Workflow ;
      prov:wasAssociatedWith
              :tool1 .

p-plan:isPreceededBy
      a       owl:ObjectProperty ;
      rdfs:domain p-plan:Step ;
      rdfs:range p-plan:Step .
