@prefix :        <http://www.ub.uni-stuttgart.de/replay#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix datacite:  <http://purl.org/spar/datacite/> .
@prefix dcterms:  <http://purl.org/dc/terms/> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix p-plan:  <http://purl.org/net/p-plan#> .
@prefix prov:    <http://www.w3.org/ns/prov#> .

prov:SoftwareAgent
      a       owl:Class ;
      rdfs:subClassOf prov:Agent .

prov:AgentInfluence
      a       owl:Class ;
      rdfs:subClassOf prov:Influence .

prov:Association
      a       owl:Class ;
      rdfs:subClassOf prov:AgentInfluence .

prov:agent
      a       owl:ObjectProperty ;
      rdfs:domain prov:AgentInfluence ;
      rdfs:range prov:Agent .

:inputResource2
      a       prov:Entity ;
      dcterms:type "inputResource" .

prov:used
      a       owl:ObjectProperty ;
      rdfs:domain prov:Activity ;
      rdfs:range prov:Entity .

prov:influenced
      a       owl:ObjectProperty ;
      rdfs:domain prov:Agent ;
      rdfs:range prov:Entity .

prov:Person
      a       owl:Class ;
      rdfs:subClassOf prov:Agent .

:Activity_step_1
      a       prov:Activity ;
      prov:influenced :Activity_step_4 .

dcterms:type
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

prov:Activity
      a       owl:Class .

:Activity_step_5
      a       prov:Activity ;
      rdfs:label "analyze" ;
      dcterms:description "Some random description for step: analyze" ;
      prov:generated :outputResource1 ;
      prov:influenced :Activity_step_6 ;
      prov:used :inputResource1 , :inputResource2 , :outputResource1 .

prov:Influence
      a       owl:Class .

prov:Plan
      a       owl:Class .

dcterms:identifier
      a       owl:DatatypeProperty , owl:ObjectProperty .

prov:Entity
      a       owl:Class ;
      owl:disjointWith prov:Person .

prov:generated
      a       owl:ObjectProperty ;
      rdfs:domain prov:Activity ;
      rdfs:range prov:Entity .

:inputResource1
      a       prov:Entity ;
      dcterms:type "inputResource" .

prov:hadRole
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

prov:wasAssociatedWith
      a       owl:ObjectProperty ;
      rdfs:domain prov:Activity ;
      rdfs:range prov:Agent .

:Activity_step_4
      a       prov:Activity ;
      rdfs:label "train" ;
      dcterms:description "Some random description for step: train" ;
      prov:generated :outputResource1 ;
      prov:influenced :Activity_step_5 ;
      prov:used :inputResource1 , :inputResource2 , :outputResource1 .

prov:wasInfluencedBy
      a       owl:ObjectProperty ;
      rdfs:domain prov:Entity ;
      rdfs:range prov:Entity .

dcterms:description
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

:Workflow
      a       prov:Plan .

:Activity_step_6
      a       prov:Activity ;
      rdfs:label "correct" ;
      dcterms:description "Some random description for step: correct" ;
      prov:generated :outputResource1 ;
      prov:used :inputResource1 , :inputResource2 , :outputResource1 .

rdfs:label
      a       owl:DatatypeProperty ;
      rdfs:range xsd:string .

prov:hadPlan
      a       owl:ObjectProperty ;
      rdfs:domain prov:Association ;
      rdfs:range prov:Plan .

:outputResource1
      a       prov:Entity ;
      dcterms:type "inputResource" , "outputResource" .

prov:qualifiedAssociation
      a       owl:ObjectProperty ;
      rdfs:domain prov:Activity ;
      rdfs:range prov:Association .

prov:Agent
      a       owl:Class .
